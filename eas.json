{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production-ios": {
      "ios": {
        "distribution": "store"
      },
      "autoIncrement": true
    },
    "production-android": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production-ios": {
      "ios": {}
    }
  }
}
